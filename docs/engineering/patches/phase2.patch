+                <input
+                  type="password"
+                  placeholder="值"
+                  value={row.value}
+                  onChange={(event) => updateRow(row.id, "value", event.target.value)}
+                />
+                <button
+                  type="button"
+                  className="danger"
+                  onClick={() => removeRow(row.id)}
+                  disabled={envRows.length === 1}
+                  aria-label={`remove env row ${index + 1}`}
