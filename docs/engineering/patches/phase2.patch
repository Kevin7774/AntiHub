+            </div>
+            {envRows.map((row, index) => (
+              <div className="env-row" key={row.id}>
+                <input
+                  type="text"
+                  placeholder="键名"
+                  value={row.key}
+                  onChange={(event) => updateRow(row.id, "key", event.target.value)}
